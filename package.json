{
  "name": "mortimer",
  "version": "2.0.1",
  "description": "rest interface for mongoose models, built on top of express",
  "homepage": "http://github.com/topliceanu/mortimer",
  "license": "MIT",
  "keywords": [
    "rest",
    "mongoose",
    "mongodb",
    "express"
  ],
  "author": "alexandru topliceanu <alexandru.topliceanu@gmail.com> (http://alexandrutopliceanu.ro)",
  "repository": {
    "type": "git",
    "url": "git://github.com/topliceanu/mortimer.git"
  },
  "bugs": {
    "url": "https://github.com/topliceanu/mortimer/issues"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "./node_modules/.bin/eslint ./lib ./test ./examples",
    "doc": "./node_modules/.bin/esdoc -c esdoc.json"
  },
  "dependencies": {},
  "devDependencies": {
    "body-parser": "1.15.2",
    "chai": "3.5.0",
    "coveralls": "2.11.12",
    "esdoc": "0.4.8",
    "eslint": "3.3.1",
    "express": "4.14.0",
    "istanbul": "0.4.4",
    "mocha": "3.0.2",
    "mocha-lcov-reporter": "1.2.0",
    "mongoose": "4.5.9",
    "supertest": "2.0.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">6.3.1"
  },
  "config": {}
}
